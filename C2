#include <stdio.h>


int main() {

    double raio, area, n = 3.14159;

    scanf("%lf", &raio);
    area = raio * raio * n;
    printf("A=%.4lf\n", area);

    return 0;
}

/* professor

#include <stdio.h>
#include<math.h>
#define PI 3.14159

int main() {

    double raio;

    scanf("%lf", &raio);
    double area = pi * pow(raio, 2);
    printf("A=%.4lf\n", area);

    return 0;
}

